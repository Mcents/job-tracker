<h1>All jobs for <strong><%= @company.name %></strong></h1>

<% @jobs.each do |job| %>
  <li><%= link_to job.title, company_job_path(@company, job) %> at <%= job.company.name %></li>
<% end %>

<p>Create contact for <%= @company.name %></p>
<%= render partial: 'contact' %>

<h2>Contact Information</h2>
<% @company.contacts.each do |contact| %>
<ul>
  <li><%= contact.name %></li>
  <li><%= contact.email %></li>
  <li><%= contact.title %></li>
</ul>
<% end %>
